APP_NAME=Caddy
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_DOMAIN=caddy.test
APP_URL=https://${APP_DOMAIN}

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=caddy
DB_USERNAME=caddy
DB_PASSWORD=cicada
DB_SOCKET=/Users/caddy/Library/Containers/com.sequel-ace.sequel-ace/Data/mysql.sock

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Redis cache and queue worker
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mailgun
MAILGUN_DOMAIN=mail.${APP_DOMAIN}
MAILGUN_SECRET=
MAILGUN_ENDPOINT=api.eu.mailgun.net

# Mailing config
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

# Pusher web sockets
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

# Laravel Mix
MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Sentry error reporting
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1

# Ignition
IGNITION_EDITOR=vscode
IGNITION_THEME=dark
REGISTER_IGNITION_COMMANDS=true

# Forge
REMOTE_HOST=zidweb
REMOTE_USER=caddy
REMOTE_PATH=/home/caddy/

# Nova
NOVA_LICENSE_KEY=
